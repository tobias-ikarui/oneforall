{
	"author": "Ayfri",
	"description": "A simple argument parser.",
	"devDependencies": {
		"prettier": "^2.3.2",
		"typescript": "^4.3.5"
	},
	"keywords": [
		"arguments",
		"args",
		"parser",
		"discord.js"
	],
	"license": "GNU",
	"main": "dist",
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": false,
		"printWidth": 160,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "es5",
		"useTabs": true
	},
	"name": "argumentorum",
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "npm run prettier && npm run build",
		"prettier": "prettier --write src *.json README.md"
	},
	"types": "dist",
	"version": "0.1.1"
}
