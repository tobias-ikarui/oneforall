{
	"author": "Ayfri",
	"bugs": {
		"url": "https://github.com/Ayfri/Better-Embed/issues"
	},
	"dependencies": {
		"discord.js": "^13.0.1"
	},
	"description": "A npm package that helps you create better Discord Embeds using discord.js.",
	"devDependencies": {
		"@types/node": "^16.4.13",
		"prettier": "^2.3.2",
		"typescript": "^4.3.5"
	},
	"engines": {
		"discord.js": ">=13.0.0"
	},
	"engineStrict": true,
	"homepage": "https://github.com/Ayfri/Better-Embed",
	"keywords": [
		"discord.js",
		"embed",
		"discord-embed",
		"better-embed",
		"embed-discord",
		"v13"
	],
	"main": "dist/index.js",
