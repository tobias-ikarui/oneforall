	"readme": "https://github.com/Ayfri/Better-Embed/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ayfri/Better-Embed"
	},
	"scripts": {
		"build": "tsc",
		"prepublishOnly": "npm run prettier && npm run build",
		"prettier": "prettier *.json *.md src --write"
	},
	"types": "dist",
	"version": "1.3.0"
}
